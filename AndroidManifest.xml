<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.informacion"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />
        
    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="com.example.informacion" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/spidey"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <uses-library android:name="android.test.runner" />
        
        <activity
            android:name="com.example.informacion.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.informacion.AltaContacto"
            android:label="@string/title_activity_alta_usuario" >
        </activity>
        <activity
            android:name="com.example.informacion.MostrarContacto"
            android:label="@string/title_activity_mostrar_usuario" >
        </activity>
        <activity
            android:name="com.example.informacion.AcercaDe"
            android:label="@string/title_activity_acerca_de" >
        </activity>
        <activity
            android:name="com.example.informacion.Login"
            android:label="@string/title_activity_login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
